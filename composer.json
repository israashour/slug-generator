{
    "name": "slg/slug-generator",
    "description": ":package_description",
    "license": "MIT",
    "authors": [
        {
            "name": "Author Name",
            "email": "author@email.com",
            "homepage": "http://author.com"
        }
    ],
    "homepage": "https://github.com/slg/slug-generator",
    "keywords": ["Laravel", "SlugGenerator"],
    "require": {
        "illuminate/support": "~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Slg\\SlugGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slg\\SlugGenerator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Slg\\SlugGenerator\\SlugGeneratorServiceProvider"
            ],
            "aliases": {
                "SlugGenerator": "Slg\\SlugGenerator\\Facades\\SlugGenerator"
            }
        }
    }
}
